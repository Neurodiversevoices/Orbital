{
  "name": "orbital",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "doctor": "node scripts/doctor.js",
    "prebuild": "npm run doctor",
    "build:ios": "npm run doctor && eas build --platform ios --profile production --non-interactive",
    "build:android": "npm run doctor && eas build --platform android --profile production --non-interactive",
    "submit:ios": "eas submit --platform ios --profile production --latest --non-interactive",
    "submit:android": "eas submit --platform android --profile production --latest --non-interactive"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@sentry/react-native": "^7.8.0",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "expo": "~54.0.30",
    "expo-build-properties": "^1.0.10",
    "expo-constants": "~18.0.12",
    "expo-crypto": "~15.0.3",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.6",
    "expo-haptics": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-print": "~14.0.3",
    "expo-router": "~6.0.21",
    "expo-sharing": "~14.0.8",
    "expo-status-bar": "~3.0.9",
    "lucide-react-native": "^0.562.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-purchases": "^8.12.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.3",
    "@types/bip39": "^3.0.4",
    "@types/react": "~19.1.0",
    "babel-preset-expo": "^54.0.9",
    "typescript": "~5.9.2"
  },
  "private": true
}
