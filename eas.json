{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "distribution": "store",
      "env": {
        "SENTRY_ALLOW_FAILURE": "true",
        "NODE_ENV": "production"
      },
      "ios": {
        "credentialsSource": "remote",
        "autoIncrement": "buildNumber",
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6757295146"
      }
    }
  }
}

